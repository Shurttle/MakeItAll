<Window x:Class="MakeItAll.ProjectSetupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MakeIt"
        mc:Ignorable="d"
        Title="ProjectSetupWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <RadioButton x:Name="rbSingleUser" Content="Проект для одного пользователя" IsChecked="True"/>
            <RadioButton x:Name="rbMultipleUsers" Content="Проект для нескольких пользователей"/>
            <!--<Button Content="Выбрать методологию" Click="OnMethodologySelectClick"/>-->
        </StackPanel>
    </Grid>
</Window>
